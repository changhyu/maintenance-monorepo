# 모바일 API 서비스 기본 설정
NODE_ENV=development
PORT=8002
DEBUG=true

# 데이터베이스 연결 설정
DATABASE_URL=postgresql://postgres:postgres@db:5432/maintenance

# 메인 API 연결 설정
MAIN_API_URL=http://api:8000

# 인증 설정
JWT_SECRET=your_secret_key_here_change_in_production
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_EXPIRES_IN=30d

# 푸시 알림 설정
PUSH_ENABLED=true
FCM_SERVER_KEY=your_firebase_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_BUNDLE_ID=com.company.maintenanceapp

# 오프라인 모드 설정
OFFLINE_SYNC_MAX_ITEMS=1000
OFFLINE_SYNC_DEBOUNCE_MS=5000

# 로깅 설정
LOG_LEVEL=debug

# 레디스 캐시 설정
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=2

# 속도 제한 설정
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=120